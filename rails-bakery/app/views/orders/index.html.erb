<h1>All previous orders</h1>
<table>
    <tr>
        <th>id</th>
        <th>total</th>
        <th>created_at</th>
    </tr>
    <% @orders.each do |order| %>
        <tr>
            <td><%= order.id %></td>
            <td><%= order.total %></td>
            <td><%= order.created_at %></td>
        </tr>
    <% end %>
</table>
